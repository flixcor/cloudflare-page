{
  "name": "cloudflare-page",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run build:ssr && vue-tsc --noEmit",
    "build:ssr": "vite build --config vite.config.ssr.ts --ssr src/entry-server.ts",
    "build:client": "vite build --config vite.config.ssr.ts --ssrManifest",
    "serve": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "^7.0.3",
    "ufo": "^0.7.9",
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.2.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "move-file-cli": "^3.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3",
    "vite": "^2.7.0-beta.8",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.3.0",
    "wrangler": "0.0.0-fsw-beta.5"
  }
}
